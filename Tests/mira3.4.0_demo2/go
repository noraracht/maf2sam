echo "Unpadded"
../../maf2sam.py demo2_out.unpadded.fasta demo2_out.maf > demo2_out.unpadded.sam
../../sam2bam.py demo2_out.unpadded.sam
echo "Unpadded Done"
echo
echo "Padded"
../../maf2sam.py demo2_out.padded.fasta demo2_out.maf > demo2_out.padded.sam
../../sam2bam.py demo2_out.padded.sam
echo "Padded Done"
